public class Customer {
	public void addCustomer(String name, String ssn, int phone, String customerId, int passwd) {
		Map<String, String> customer = new HashMap<String, String>();
		customer1.add(name);
		customer1.add(ssn);
		customer1.add(phone);
		customer1.add(customerId);
		customer1.add(passwd);
		
		Set customer1 = new HashSet();
		customer1.add(name);
		customer1.add(ssn);
