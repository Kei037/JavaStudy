package com.varxyz.jy200.mod002;

public class MyDate {
	private int day;
	private int month;
	private int year;

	public MyDate(int day, int month, int year) {
		day = d;
		month = m;
		year = y;
	}
	
	public void setDay(int d) {
		day = d;
	}
	
	public String toString() {
		return "day = " + day + ", month = " + month + ", year = " + year;
	}

}
