package com.varxyz.jy200.mod007;

public class Book extends Libary {
	public void Book(String book) {
		this.book = book;
	}

	String book;
	
	public void addBook(Book book) {
		this.book = book;
		bookList[count] = this.book;
		count++;
	}
	
	public void getBook(int serial) {
		System.out.println(bookList[serial]);
	}
}
