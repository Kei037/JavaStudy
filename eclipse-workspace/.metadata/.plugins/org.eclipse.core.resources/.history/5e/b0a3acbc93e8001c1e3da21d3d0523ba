package com.varxyz.jy200.mod003;

public class CountTest {
	
	public static void main(String[] args) {
		
		int result;
		result = 0;
		
		for (int i = 2; i < 1001; i++) {
			for (int j = 2; j <= i ; j++) {
				if ( i % j == 0 ) {
					break;
				}else if( i == j ) {
					++result;
				}
			}
		}
		
		System.out.println(result);

	}
}