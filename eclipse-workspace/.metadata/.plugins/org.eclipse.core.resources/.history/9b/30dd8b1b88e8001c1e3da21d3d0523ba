package com.varxyz.jy200.mod003;

public class CountTest {

	static int result = 0;
	
	private void CountTest(int x) {
		for (int i = 1; i < 1001; i++) {
			if ( 1 % x == 0 && x % x == 0 ) {
				result++;
			}
		}
		
	}
	
	public static void main(String[] args) {
		
		CountTest t = new CountTest();
		t.CountTest(1000);
		System.out.println(result);
		

	}
}
