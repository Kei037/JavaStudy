package com.varxyz.banking.domain;

import java.util.HashMap;
import java.util.HashSet;
import java.util.Map;
import java.util.Set;

public class Customer {
	public void addCustomer(String name, String ssn, String phone, String customerId, String passwd) {
		Map<String, String> customer = new HashMap<String, String>();
		customer.put(Integer.valueOf(0), name);
		customer.put(ssn);
		customer.put(phone);
		customer.put(customerId);
		customer.put(passwd);
		
//		Set customer1 = new HashSet();
//		customer1.add(name);
//		customer1.add(ssn);
//		customer1.add(phone);
//		customer1.add(customerId);
//		customer1.add(passwd);
		
		
		System.out.println(customer1);
	}
	public static void main(String[] args) {
		Customer a = new Customer();
		a.addCustomer("홍길동", "111111-3254687", "");
	}
}
