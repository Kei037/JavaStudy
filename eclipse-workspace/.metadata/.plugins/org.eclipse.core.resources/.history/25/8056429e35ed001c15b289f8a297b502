package com.varxyz.jy200.mod007;

public class Libary {
	private static Book book;
	int serial;
	int count;
	
	public Book[] bookList = new Book[100];
	
	private Libary() {
	}
	
    public Book addBook(Book book) {
    	return book;
	}
	
	public static void main(String[] args) {
		Libary gg = new Libary();
		gg.addBook("s");
	}
}
