package com.varxyz.jy200.mod003;

public class TestRun {
	
	private static int result = 0;
	
	public static void main (String[] args) {
		for (int i = 2; i < 10; i++) {
			System.out.println("");
			for (int j = 1; j < 10; j++) {
				System.out.print(i + " * " + j + " = " + i * j);
				System.out.println("");
			}
			System.out.println("");
		}
	}

}
